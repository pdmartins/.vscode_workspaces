using Hapvida.TI.VoiceTranscription.Domain.Enums;

namespace Hapvida.TI.VoiceTranscription.Domain.Dtos
{
    public class RowDto
    {
        public string WorkId { get; set; } = "";
        public string EncounterId { get; set; } = "";
        public List<string> Symptoms { get; set; } = new List<string>();
        public string Transcription { get; set; } = "";
        public List<SuggestedCid> SuggestedCIDs { get; set; } = new();
        public WorkStatus WorkStatus { get; set; }
        public List<string> ReadyFields { get; set; } = new List<string>();
        public string CreatedAt { get; set; } = string.Empty;
    }

}
